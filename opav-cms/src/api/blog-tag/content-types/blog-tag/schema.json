{
  "kind": "collectionType",
  "collectionName": "blog_tags",
  "info": {
    "singularName": "blog-tag",
    "pluralName": "blog-tags",
    "displayName": "Blog Tag"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": false
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true,
      "unique": true
    },
    "posts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog-post.blog-post",
      "mappedBy": "tags"
    }
  }
}
